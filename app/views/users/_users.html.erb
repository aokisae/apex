<% if users.any? %> 
  <ul class="list-unstyled">
    <% users.each do |user| %>
    <div class="media-body">
          <div>
            <%= user.name %>
          </div>
    <% if logged_in? %>
      <% if current_user.admin? %>
      <%= link_to "Delete", user, method: :delete, data: { confirm: "You sure?" }, class: 'btn btn-danger btn-sm' %>
      <% end %>
    <% end %>
    </div>
    <% end %>
  </ul>
<% end %>